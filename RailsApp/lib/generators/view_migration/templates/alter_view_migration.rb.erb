class <%= migration_class_name %> < ActiveRecord::Migration
  def up
    drop_view :<%= view_name %>
    create_view :<%= view_name %>, load_sql(<%= migration_number %>, '<%= view_name %>')
  end

  def down
    drop_view :<%= view_name %>
    create_view :<%= view_name %>, load_sql(<%= last_sql_number %>, '<%= view_name %>')
  end
end

